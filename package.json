{
  "name": "ads",
  "version": "0.0.0",
  "description": "Implementation of various algorithms and data structures on ES2015. With support for TypeScript.",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "build": "npm-run-all clean build:cjs",
    "build:cjs": "npm-run-all clean:cjs compile:cjs",
    "build:es": "echo \"Error: not implemented\" && exit 1",
    "build:bundle": "echo \"Error: not implemented\" && exit 1",
    "build:types": "npm-run-all clean:types compile:types",
    "clean": "shx rm -rf ./dist",
    "clean:cjs": "shx rm -rf ./dist/cjs",
    "clean:es": "echo \"Error: not implemented\" && exit 1",
    "clean:bundle": "echo \"Error: not implemented\" && exit 1",
    "clean:types": "shx rm -rf ./dist/types",
    "compile:cjs": "tsc -p ./tsconfig.cjs.json",
    "compile:es": "echo \"Error: not implemented\" && exit 1",
    "compile:bundle": "echo \"Error: not implemented\" && exit 1",
    "compile:types": "tsc -p ./tsconfig.types.json",
    "lint": "npm-run-all --parallel lint:*",
    "lint:spec": "tslint -c tslint.json \"./spec/**/*.ts\"",
    "lint:src": "tslint -c tslint.json \"./src/**/*.ts\"",
    "test": "nyc mocha \"./spec/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-myznikov/ads.git"
  },
  "keywords": [
    "algorithms",
    "data structures"
  ],
  "author": "Alexander Myznikov",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {},
  "types": "./dist/types/index.d.ts",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/index.ts",
      "**/*.spec.ts"
    ]
  }
}
