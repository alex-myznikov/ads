{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "brace-style": "error",
    "camelcase": "error",
    "comma-dangle": ["error", "always-multiline"], 
    "comma-spacing": "error",
    "comma-style": "error",
    "consistent-return": "error", 
    "curly": ["error", "multi"], 
    "dot-notation": "error", 
    "eol-last": "error",
    "eqeqeq": "error", 
    "generator-star-spacing": ["error", "before"],
    "guard-for-in": "error", 
    "indent": ["error", 2],
    "key-spacing": "error",
    "max-len": ["error", 120, 2],
    "new-cap": "error",
    "new-parens": "error",
    "newline-after-var": "error",
    "no-alert": "error", 
    "no-caller": "error", 
    "no-catch-shadow": "error",
    "no-console": "error", 
    "no-else-return": "error", 
    "no-empty-class": "error", 
    "no-empty-label": "error", 
    "no-eq-null": "error", 
    "no-eval": "error", 
    "no-extra-bind": "error", 
    "no-extra-parens": "error", 
    "no-implied-eval": "error", 
    "no-label-var": "error",
    "no-labels": "error", 
    "no-lone-blocks": "error", 
    "no-lonely-if": "error",
    "no-loop-func": "error",
    "no-multi-spaces": "error", 
    "no-multi-str": "error", 
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "no-native-reassign": "error", 
    "no-negated-in-lhs": "error", 
    "no-nested-ternary": "error",
    "no-new-func": "error", 
    "no-new-wrappers": "error", 
    "no-new": "error", 
    "no-param-reassign": "error", 
    "no-proto": "error", 
    "no-reserved-keys": "error", 
    "no-return-assign": "error", 
    "no-script-url": "error", 
    "no-self-compare": "error", 
    "no-sequences": "error",
    "no-shadow": "error",
    "no-spaced-func": "error",
    "no-throw-literal": "error", 
    "no-trailing-spaces": "error",
    "no-undef-init": "error",
    "no-undefined": "error",
    "no-unused-expressions": "error",
    "no-use-before-define": "error", 
    "no-var": "error",
    "no-void": "error", 
    "no-warning-comments": ["error", {"terms": ["todo", "fixme"], "location": "start"}], 
    "no-wrap-func": "error",
    "operator-assignment": "error",
    "padded-blocks": ["error", {"classes": "always", "blocks": "never", "switches": "never"}],
    "quote-props": ["error", "as-needed"],
    "quotes": ["error", "single"],
    "radix": "error", 
    "semi-spacing": "error",
    "semi": "error",
    "space-after-keywords": "error",
    "space-before-blocks": "error",
    "space-before-function-paren": ["error", {"anonymous": "always", "async": "always", "named": "never"}],
    "space-in-brackets": "error",
    "space-in-parens": "error",
    "space-infix-ops": "error",
    "space-return-throw-case": "error",
    "space-unary-ops": "error",
    "spaced-line-comment": "error",
    "valid-jsdoc": "error", 
    "vars-on-top": "error", 
    "wrap-iife": "error", 
    "yoda": "error",
  }
}
