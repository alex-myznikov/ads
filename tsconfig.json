{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "ads": ["./src/index.ts"],
    },
    "moduleResolution": "node",
    "target": "es6",
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "stripInternal": false,
    "downlevelIteration": true,
    "outDir": "./dist"
  }
}